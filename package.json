{
  "name": "lemon-tab-page",
  "private": true,
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:edge": "wxt -b edge",
    "build": "vue-tsc --build && wxt build",
    "build:firefox": "vue-tsc --build && wxt build -b firefox",
    "build:edge": "vue-tsc --build && wxt build -b edge",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "postinstall": "wxt prepare",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint:style": "stylelint **/*.{css,scss,sass,less,vue,html} --cache --fix",
    "lint": "run-s lint:*",
    "format": "prettier assets/ entrypoints/ locales/ shared/ types/ scripts/ --write -u"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "dompurify": "^3.2.7",
    "jinrishici": "^1.0.6",
    "localforage": "^1.10.0",
    "pinia": "^3.0.3",
    "uuid": "^13.0.0",
    "vue": "^3.5.22",
    "vue-draggable-plus": "^0.6.0"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node24": "^24.0.1",
    "@types/chrome": "^0.1.14",
    "@types/lodash": "^4.17.20",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.6.0",
    "@types/webextension-polyfill": "^0.12.3",
    "@vicons/antd": "^0.13.0",
    "@vicons/fa": "^0.13.0",
    "@vicons/fluent": "^0.13.0",
    "@vicons/material": "^0.13.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.9.0",
    "@wxt-dev/module-vue": "^1.0.3",
    "@wxt-dev/webextension-polyfill": "^1.0.0",
    "element-plus": "^2.11.4",
    "eslint": "^9.36.0",
    "eslint-plugin-oxlint": "^1.19.0",
    "eslint-plugin-vue": "^10.5.0",
    "fast-glob": "^3.3.3",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "i18next-vue": "^5.3.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.19.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "stylelint": "^16.24.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "unplugin-vue-markdown": "^29.2.0",
    "vite": "npm:rolldown-vite@^7.1.14",
    "vite-plugin-i18next-loader": "^3.1.3",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.1.0",
    "webextension-polyfill": "^0.12.0",
    "wxt": "^0.20.11"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@^7.1.14",
      "uuid": "^13.0.0",
      "@vitejs/plugin-vue": "^6.0.1",
      "tmp": "^0.2.5"
    }
  }
