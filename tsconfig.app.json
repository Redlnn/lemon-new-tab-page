{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./.wxt/i18n/**/*",
    "./.wxt/types/**/*",
    "./entrypoints/**/*",
    "./types/**/*",
    "./utils/**/*"
  ],
  "exclude": ["./.output", "./utils/optimizeDeps.ts"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/newtab/*": ["./entrypoints/newtab/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "vue"
  }
}
