{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    ".wxt/i18n/**/*",
    ".wxt/types/**/*",
    "entrypoints/**/*",
    "entrypoints/**/*.vue",
    "types/**/*",
    "shared/**/*"
  ],
  "exclude": [".output", "scripts/**/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@newtab/*": ["./entrypoints/newtab/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "vue"
  }
}
